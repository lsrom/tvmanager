<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cz.lsrom.tvmanager.controller.PreferencesController">
   <children>
      <BorderPane prefHeight="600.0" prefWidth="400.0">
         <center>
            <TabPane prefHeight="342.0" prefWidth="683.0" tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER">
              <tabs>
                <Tab text="General">
                  <content>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="600.0" prefWidth="400.0">
                           <children>
                              <VBox prefHeight="371.0" prefWidth="400.0">
                                 <children>
                                    <Label prefHeight="16.0" prefWidth="402.0" text="Select default file chooser open location.">
                                       <VBox.margin>
                                          <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
                                       </VBox.margin>
                                       <padding>
                                          <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
                                       </padding>
                                    </Label>
                                    <HBox prefHeight="20.0" prefWidth="400.0" VBox.vgrow="NEVER">
                                       <children>
                                          <TextField fx:id="txtDefaultOpenLocation" editable="false" text="Default Open Location" HBox.hgrow="ALWAYS">
                                             <HBox.margin>
                                                <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
                                             </HBox.margin>
                                          </TextField>
                                          <Button fx:id="btnChooseDefaultOpenLocation" minWidth="70.0" mnemonicParsing="false" text="Choose">
                                             <HBox.margin>
                                                <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
                                             </HBox.margin>
                                          </Button>
                                       </children></HBox>
                                    <Label prefHeight="16.0" prefWidth="403.0" text="Select your TV directory.">
                                       <VBox.margin>
                                          <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
                                       </VBox.margin>
                                       <padding>
                                          <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
                                       </padding>
                                    </Label>
                                    <HBox prefHeight="16.0" prefWidth="400.0" VBox.vgrow="NEVER">
                                       <children>
                                          <TextField fx:id="txtTvDirectory" editable="false" text="TV Directory" HBox.hgrow="ALWAYS">
                                             <HBox.margin>
                                                <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
                                             </HBox.margin>
                                          </TextField>
                                          <Button fx:id="btnChoseTvDirectory" minWidth="70.0" mnemonicParsing="false" text="Choose">
                                             <HBox.margin>
                                                <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
                                             </HBox.margin>
                                          </Button>
                                       </children>
                                    </HBox>
                                    <Label prefHeight="22.0" prefWidth="398.0" text="Select directory in which you download new shows">
                                       <padding>
                                          <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
                                       </padding>
                                       <VBox.margin>
                                          <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
                                       </VBox.margin>
                                    </Label>
                                    <HBox prefHeight="0.0" prefWidth="400.0" VBox.vgrow="NEVER">
                                       <children>
                                          <TextField fx:id="txtDownloadDirectory" editable="false" text="TV Download Directory" HBox.hgrow="ALWAYS">
                                             <HBox.margin>
                                                <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
                                             </HBox.margin>
                                          </TextField>
                                          <Button fx:id="btnDownloadDirectory" minWidth="70.0" mnemonicParsing="false" text="Choose">
                                             <HBox.margin>
                                                <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
                                             </HBox.margin>
                                          </Button>
                                       </children>
                                    </HBox>
                                    <Label prefHeight="22.0" prefWidth="373.0" text="Select what file extension should be loaded" VBox.vgrow="ALWAYS">
                                       <VBox.margin>
                                          <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
                                       </VBox.margin>
                                       <padding>
                                          <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
                                       </padding>
                                    </Label>
                                    <HBox prefHeight="7.0" prefWidth="400.0" VBox.vgrow="NEVER">
                                       <children>
                                          <TextField fx:id="txtFileExtensions" HBox.hgrow="ALWAYS">
                                             <HBox.margin>
                                                <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
                                             </HBox.margin>
                                          </TextField>
                                          <Button fx:id="btnFileExtensions" minWidth="70.0" mnemonicParsing="false" text="Select">
                                             <HBox.margin>
                                                <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
                                             </HBox.margin>
                                          </Button>
                                       </children>
                                    </HBox>
                                    <Label text="Skip files containing these substring (case insensitive)">
                                       <VBox.margin>
                                          <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
                                       </VBox.margin>
                                       <padding>
                                          <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
                                       </padding>
                                    </Label>
                                    <HBox prefHeight="100.0" prefWidth="200.0">
                                       <children>
                                          <TextField fx:id="txtSkipFiles" HBox.hgrow="ALWAYS">
                                             <HBox.margin>
                                                <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
                                             </HBox.margin>
                                          </TextField>
                                          <Button fx:id="btnSkipFiles" minWidth="70.0" mnemonicParsing="false" text="OK">
                                             <HBox.margin>
                                                <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
                                             </HBox.margin>
                                          </Button>
                                       </children>
                                    </HBox>
                                 </children>
                              </VBox>
                           </children></AnchorPane>
                  </content>
                </Tab>
                <Tab text="Automatic Renaming">
                  <content>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="600.0" prefWidth="400.0">
                           <children>
                              <VBox prefHeight="200.0" prefWidth="400">
                                 <children>
                                    <TextArea fx:id="renameText" prefHeight="100.0" text="Use these tokens in rename format to get the filename you want. Numbers after the '%' sign are for padding numbers with zeroes. For example, %3s will make season number always three digits long with leading zeroes." wrapText="true" VBox.vgrow="SOMETIMES">
                                       <VBox.margin>
                                          <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
                                       </VBox.margin>
                                       <padding>
                                          <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                       </padding>
                                    </TextArea>
                                    <ListView fx:id="renameTokensList" minHeight="160.0" prefHeight="400.0" prefWidth="380.0">
                                       <VBox.margin>
                                          <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
                                       </VBox.margin>
                                       <padding>
                                          <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                       </padding></ListView>
                                    <HBox prefHeight="50.0" prefWidth="400.0">
                                       <children>
                                          <Label text="Rename Format">
                                             <HBox.margin>
                                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                             </HBox.margin>
                                          </Label>
                                          <TextField fx:id="txtRenameFormat" HBox.hgrow="ALWAYS">
                                             <HBox.margin>
                                                <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
                                             </HBox.margin></TextField>
                                          <Button fx:id="btnRenameFormat" mnemonicParsing="false" text="OK">
                                             <HBox.margin>
                                                <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
                                             </HBox.margin></Button>
                                       </children>
                                       <VBox.margin>
                                          <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
                                       </VBox.margin>
                                    </HBox>
                                    <CheckBox fx:id="checkSkipEmptyResolutionToken" mnemonicParsing="false" text="Skip empty resolution token?">
                                       <VBox.margin>
                                          <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
                                       </VBox.margin>
                                    </CheckBox>
                                    <CheckBox fx:id="checkAggressivelySkipEmptyResolutionToken" mnemonicParsing="false" text="Aggressively skip empty resolution token?">
                                       <VBox.margin>
                                          <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
                                       </VBox.margin>
                                    </CheckBox>
                                    <HBox prefHeight="100.0" prefWidth="200.0">
                                       <children>
                                          <CheckBox fx:id="checkSaveRenameHistory" mnemonicParsing="false" text="Save rename history?">
                                             <HBox.margin>
                                                <Insets bottom="5.0" left="10.0" right="10.0" top="10.0" />
                                             </HBox.margin>
                                          </CheckBox>
                                          <ChoiceBox fx:id="choiceRenameHistory" disable="true" prefWidth="150.0" HBox.hgrow="ALWAYS">
                                             <HBox.margin>
                                                <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
                                             </HBox.margin>
                                          </ChoiceBox>
                                       </children>
                                    </HBox>
                                    <CheckBox fx:id="checkDownloadDirectory" mnemonicParsing="false" text="Always load from Download directory on startup">
                                       <VBox.margin>
                                          <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
                                       </VBox.margin>
                                    </CheckBox>
                                 </children>
                              </VBox>
                           </children></AnchorPane>
                  </content>
                </Tab>
              </tabs>
            </TabPane>
         </center>
         <top>
            <HBox prefHeight="30.0" prefWidth="400.0" BorderPane.alignment="CENTER" />
         </top>
      </BorderPane>
   </children>
</AnchorPane>
